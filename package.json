{
  "name": "@ask-utils/handlers",
  "version": "1.1.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "tsc",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint 'libs/**/*.{ts,tsx}'",
    "postversion": "git push origin $(git describe --abbrev=0 --tags)",
    "doc": "typedoc --out docs libs/",
    "test:watch": "jest --watch",
    "test:dev": "jest --watch --silent=false --verbose false --coverage"
  },
  "bugs": {
    "url": "https://github.com/ask-utils/handlers/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ask-utils/handlers.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "Alexa",
    "ASK SDK",
    "Proactive Event",
    "ask-utils"
  ],
  "author": "Hidetaka Okamoto <info@wp-kyoto.net> (https://wp-kyoto.net)",
  "license": "MIT",
  "dependencies": {
    "ask-sdk": "^2.3.0",
    "ask-sdk-core": "^2.0.7",
    "ask-sdk-model": "^1.3.1",
    "ask-sdk-runtime": "^2.4.0",
    "ask-utils": "^1.5.1",
    "axios": "^0.19.0",
    "class-resolver": "^1.1.0",
    "moment": "^2.24.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^1.1.1",
    "@typescript-eslint/parser": "^1.1.1",
    "@typescript-eslint/typescript-estree": "^1.1.1",
    "eslint": "^5.15.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "ts-jest": "^23.10.5",
    "typedoc": "^0.14.1",
    "typescript": "^3.3.3333"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/__tests__/*/*.+(ts|tsx|js)"
    ]
  },
  "homepage": "https://github.com/ask-utils/handlers#readme",
  "directories": {
    "doc": "docs"
  }
}
